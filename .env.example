# URL for the MCP Obsidian vault server
MCP_VAULT_URL=http://mcp_obsidian:3333

# URL for the existing MCP server
MCP_SERVER_URL=http://mcp_server:3333

# Optional: Path to the vault inside the container (for reference)
# VAULT_PATH=/vault

# API Keys for external services
OPENROUTER_API_KEY=sk-or-your_key_here
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CSE_ID=your_google_cse_id

# Secret for MCP Obsidian vault server
MCP_SECRET=your_secret_token_here


# Auth token for frontend API
MCP_API_AUTH_TOKEN=your_auth_token_here
VITE_MCP_API_AUTH_TOKEN=your_auth_token_here
